<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!-- bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"
    integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <!-- navbar CSS -->
    <link rel="stylesheet" href="static/css/navbar.css">
    <!-- quick_search CSS -->
    <link rel="stylesheet" href="static/css/show_data.css">
    <!-- quick_search_sidebar CSS -->
    <link rel="stylesheet" href="static/css/quick_search_sidebar.css">
    <!-- jquery -->
    

</head>

<!-- nav바 아래에 body가 오도록 height를 찾고 body margin top을 변경해준다 -->
<script>
    var old_nav_height = $("#navbar").height();
    $('body').css("margin-top", old_nav_height);
    var new_nav_height = 0;
    
    const change_height = function () {
        new_nav_height = $("#navbar").height();
        if (old_nav_height != new_nav_height){
            $('body').css("margin-top", new_nav_height);
            old_nav_height = new_nav_height;
        }
    }
    const timeoutId = setInterval(change_height, 1000);
</script>
<body>
    <%-include("navbar") %>
    <%-include("quick_search") %>
    <div class="row">
        <div class="col-9">
            <div data-bs-spy="scroll" data-bs-target="#data_scrollspy" data-bs-offset="0">
                <%for(var i=0; i< artbox_data[0].length; i++) { %>
                    <h3 id=<%= artbox_data[0][i] %>><%= artbox_data[0][i] %></h3>
                    <hr>
                    <%for(var j=0; j< artbox_data[1][i].length; j++) { %>
                        <h5 id=<%= artbox_data[1][i][j] %>><%= artbox_data[1][i][j] %></h5>
                        <hr>
                    <% } %>
                <% } %>
            </div>    
        </div>
        <div class="col-3">
            <%-include("scrollspy")%>
        </div>
            
</body>
</html>