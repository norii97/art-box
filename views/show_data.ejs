<!-- nav바 아래에 body가 오도록 height를 찾고 body margin top을 변경해준다 -->
<script>

    function mod_scroll() {
        console.log('workdinsdf')
        const change_height = function () {
            var nav_height = $("#navbar").height();
            c_height = -nav_height - 15;
            window.scrollBy(0, c_height);
        }
        const timeoutId = setTimeout(change_height, 1000);
    }

</script>

<!-- 클릭시 text와 일치하는 id의 요소를 찾아낸 후 그 요소의 위치로 이동 -->
