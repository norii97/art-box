</script> -->


    
<body>
    <%-include("navbar") %>
    <%-include("quick_search") %>
    <% var pic_count = 0;%>
    <div class="row"">
        <div class="col-9" id="img_container">
            <div data-bs-target="#navbar-example3" data-bs-offset="0">
                <%for(var i=0; i< artbox_data[0].length; i++) { %>
                    <h3 class="era_title" id=<%= artbox_data[0][i] %>><%= artbox_data[0][i] %></h3>
                    <hr>
                    <%for(var j=0; j< artbox_data[1][i].length; j++) { %>
                        <h5 class="era_title" id=<%= artbox_data[1][i][j] %>><%= artbox_data[1][i][j] %></h5>
                        <hr>
                            <div class="row">
                                <% for(var three=0; three<3; pic_count++, three++) { %>
                                    <% if(pic_count >= artbox_img.length) {%>
                                        <%continue %>
                                    <% } %>
                                    <div class="col-4">
                                        <img class="museum_img" src="static/img/artbox/<%= artbox_img[pic_count]; %>">
                                    </div>
                                <% } %>
                            </div>

                    <% } %>
                <% } %>
            </div>    
        </div>
        <div class="col-3 scrolspy">
            <%-include("scrollspy")%>
        </div>
</body>
</html>