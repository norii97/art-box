    <!-- 사진 클릭시 navbar 숨김처리 -->
    <script>
        document.addEventListener('click', nav_fade);
        document.addEventListener('scroll', nav_fade);
        
        function nav_fade(){
            if($("div").hasClass("swal2-container")){
                $("#navbar").fadeOut(500);
            }else{
                $("#navbar").fadeIn(500);
            }
        }
