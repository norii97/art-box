<style>
/** 배너 */
#bannerDiv {
    display: block;
    position: relative;
    height: 97vh;
}
.first_img {
    margin-top: 5%;
    opacity: 0.6;
    filter: brightness(100%); 
    width: 100%;
    height: 70vh;
    position: relative;
    overflow: hidden;
    z-index: 10;
}

.banner_text {
    position: absolute;
    top: 80%;
    left: 7%;
    font-size: calc(0.75em + 5.5vw);
    color: black;
    font-family: Georgia, 'Times New Roman', Times, serif;
    z-index: 100;
}
/* bannerDiv 반응형 */
@media screen and (max-width: 426px) {
    #bannerDiv{
        height: 760px;
    }
    .banner_text {
        top: 94.8%;
    }
}
@media screen and (min-width: 426px) and (max-width: 768px) {
    #bannerDiv{
        height: 800px;
    }
    .banner_text {
        top: 92%;
    }
}
@media screen and (min-width: 768px) and (max-width: 992px) {
    #bannerDiv{
        height: 860px;
    }
    .banner_text {
        top: 86%;
    }
}

.News_card {
    margin-right: 6%;
    margin-left: 18%;
    margin-top: -450px;
}

.exhibit_menu {
    position:sticky;
    display: inline-block;
    top: 0;
    color: black;
    width: 12%;
    margin-left: 3%;
    margin-top: 10%;
    top: 150px;
    bottom: 5%;
    z-index: 999;
}

.exhibit_menu2 {
    display: none;
    text-align: center;
    letter-spacing: 7px;
    font-size: calc(0.75em + 1vw);
}

    /* Card img hover 커지는 효과 */
.card-img-top {
    transition: all 0.1s linear;
}

.img_eff {
    overflow: hidden;
}

.img_eff:hover .card-img-top {
    transform: scale(1.05);
}

.card-body {
    padding: 7%;
    line-height: 30px;
}

/* Card News Title hover 색 변화 효과*/
.card-body a:hover {
    color: rgb(110, 37, 37);
    font-weight: bold;

}

.card-body a {
    text-decoration-line: none;
    color: black;
}

.exhibit_menu a:hover {
    color: rgb(110, 37, 37);
    font-weight: bold;

}

.exhibit_menu a {
    text-decoration-line: none;
    color: black;
}

.card-title {
    font-weight: bolder;
    width: auto;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}



/* 반응형 */
@media(max-width:992px){
    .card-title{
        all: none;
    }
    .card-text{
        display: none;
    }
}

@media(max-width:800px){
    .exhi_all {
    height: auto;
    }
    .News_card {
        margin-top: 0%;
    }
    .exhibit_menu{
        display: none;
    }
    .exhibit_menu2 {
        display: block;
    }
    .News_card{
        margin-left: 4%;
    }
